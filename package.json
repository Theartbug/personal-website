{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "A personal branding website for Ash Provost",
  "repository": "https://github.com/Theartbug/personal-website.git",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --config webpack.prod.js",
    "build": "npm run build:types && npm run build:js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/svgTransform.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  },
  "author": "Ash Provost",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-scroll": "^1.8.2",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^1.5.7",
    "emotion": "^9.2.6",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jest-dom": "^3.8.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "^1.1.7",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^1.1.1",
    "postcss": "^8.4.31",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "precss": "^3.1.0",
    "prop-types": "^15.7.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.7",
    "typescript": "^3.9.7",
    "url-loader": "^0.6.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.8.3",
    "react-icons": "^3.11.0",
    "react-image-gallery": "^1.0.9",
    "react-intersection-observer": "^8.0.5",
    "react-scroll": "^1.8.2",
    "react-spinners": "^0.9.0"
  }
}
